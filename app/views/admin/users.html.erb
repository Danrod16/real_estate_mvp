<div class="row table">
  <% @users.each do |user| %>

      <!-- sidebar for dashboard -->
      <%= render 'shared/sidebar' %>
      <!-- sidebar end -->
      <div class="col-sm-3">
        <%= user.first_name %>
        <%= user.last_name %>
      </div>
      <div class="col-sm-3">
        <%= user.email %>
      </div>
      <div class="col-sm-3">
        <%= user.created_at.strftime("%d %m %y") %>
      </div>
      <div class="col-sm-3">
        <%= link_to "View", profile_path(user.id) %>
      </div>

  <% end %>
</div>
